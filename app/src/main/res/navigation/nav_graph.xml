<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph">

    <fragment
        android:id="@+id/loginFragment"
        android:name="com.jadevelopers.eden.ui.login.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment_to_starSessionFragment2"
            app:destination="@id/starSessionFragment" />
        <action
            android:id="@+id/action_loginFragment_to_registerFragment"
            app:destination="@id/registerFragment" />
    </fragment>
    <fragment
        android:id="@+id/starSessionFragment"
        android:name="com.jadevelopers.eden.StarSessionFragment"
        android:label="fragment_star_session"
        tools:layout="@layout/fragment_star_session" />
    <fragment
        android:id="@+id/ageRestrictionFragment"
        android:name="com.jadevelopers.eden.AgeRestrictionFragment"
        android:label="fragment_age_restriction"
        tools:layout="@layout/fragment_age_restriction" >
        <action
            android:id="@+id/action_ageRestrictionFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
    </fragment>
    <fragment
        android:id="@+id/registerFragment"
        android:name="com.jadevelopers.eden.RegisterFragment"
        android:label="fragment_register"
        tools:layout="@layout/fragment_register" />
</navigation>